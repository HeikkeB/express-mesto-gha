{
  "extends": [
    "airbnb-base",
    "plugin:import/recommended"
  ],

  "settings":{
    "babel-module": {
      "extensions": [".js", ".jsx"],
    }
  },

  "rules": {
    "import/extensions": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "linebreak-style": 0
  }
}
